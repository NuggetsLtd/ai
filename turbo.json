{
  "$schema": "https://turbo.build/schema.json",
  "globalPassThroughEnv": ["GEMINI_API_KEY"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**", "dist/**"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "a2a:cli": {
      "persistent": false,
      "cache": false
    },
    "agents:coder": {
      "persistent": true,
      "cache": false
    }
  }
}
